<div class="cars-container">
<% @dogs.each do |dog| %>
  <div>
    <h2> Name: <%= dog.name %></h2>
    <h2> Size: <%= dog.size %></h2>
    <h2> Age: <%=  dog.age %></h2>
    <h2> Description: <%=  dog.description %></h2>
    <h2> Likes: <%=  dog.likes %></h2>
    <h2> Address: <%=  dog.address %></h2>
    <h2> Gender: <%=  dog.gender%></h2>
      <%= link_to 'View dog', dog_path(dog), class: "btn btn-flat" %>
      <p> ---------------------------- </p>
  </div>
    <% end %>
    <%= link_to 'Map', dogs_map_path, class: "btn btn-flat" %>
</div>
